[alias]
  s  = status -s -b
  bl = branch --list -a
  bd = branch -d
  l  = log --pretty='format:%C(yellow)%h %C(blue)%ad %C(reset)%s%C(red)%d %C(green)%an%C(reset), %C(cyan)%ar' --date=short --max-count=20
  la = log --pretty='format:%C(yellow)%h %C(blue)%ad %C(reset)%s%C(red)%d %C(green)%an%C(reset), %C(cyan)%ar' --date=short
  co = checkout
  ca = commit -a
  f  = fetch -p
  undo = checkout --
